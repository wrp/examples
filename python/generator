#!/usr/bin/env python



import itertools

def foo():
	while True:
		yield 0
		yield 0
		yield 1
		yield 1

k = []
for i in foo():
    k.append(i)
    if len(k) > 9:
        break

print( "k = {}", k)

#m = itertools.islice(foo(), 10)

x = foo()
m = [next(x) for _ in xrange(10)]

print( "m = {}", m)
