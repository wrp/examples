#!/usr/bin/env perl
# A random collection of symbols.

use Unicode::UCD "charinfo";
use Data::Dumper;


@samples = (
	'f09f9b91',  # A stop sign: ðŸ›‘  x1f6d1
	'e29c8b',    # A yellow hand  âœ‹
	'f09f8cb9',  # A rose ðŸŒ¹
);
printf "%8s: %s\n", $_, pack "H*", $_ foreach @samples;
print Dumper(charinfo(0x1f6d1));

# use open ":std", ":encoding(UTF-8)";
binmode STDOUT, ':encoding(UTF-8)';

print "0x1f6d1: ";
print chr(0x1f6d1) . "\n";

for (my $i=0x2B00; $i < 0x2B56;) {
	print chr($i++) . "  ";
	print "\n" if ($i % 16 == 0);
}
print "\n";


# perl -MUnicode::UCD=charinfo -MData::Dumper -e 'print Dumper(charinfo(0x1f6d1))'
