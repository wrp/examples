#!/usr/bin/env perl
# A random collection of symbols.

my $base = 'f09f9b91'; # A stop sign: ðŸ›‘
@samples = (
	$base,
	'e29c8b',    # A yellow hand  âœ‹
	'f09f8cb9',  # A rose ðŸŒ¹
);

for(my $i=15; $i < 40; $i++){
	my $v = hex($base) + $i;
	@a = (sprintf("%x", $v));
	push @samples, @a;
}

printf "%8s: %s\n", $_, pack "H*", $_ foreach @samples;
